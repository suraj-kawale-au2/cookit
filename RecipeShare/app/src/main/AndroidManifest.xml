<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.nikhil.recipeshare">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".UserLogin"></activity>
        <activity android:name=".ChiefLogin"></activity>
        <activity android:name=".AdminLogin"></activity>
        <activity android:name=".AdminRegister"></activity>
        <activity android:name=".ChiefRegister"></activity>
        <activity android:name=".UserRegister"></activity>
    </application>

</manifest>